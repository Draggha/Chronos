  "license": "MIT",
  "private": true,
  "version": "0.0.1",
  "type": "app",
  "scripts": {
    "start": "node node_modules/hoodie-server/bin/start",
    "dev": "node_modules/.bin/grunt",
    "build": "node_modules/.bin/grunt prod",
    "test": "node_modules/.bin/grunt test:unit",
    "test:ui": "node_modules/.bin/grunt test:ui",
    "postinstall": "jspm install",
    "lint": "standard --format www/src/js/app/app.js"
  },
  "dependencies": {
    "hoodie-plugin-appconfig": "^2.0.1",
    "hoodie-plugin-email": "^1.0.0",
    "hoodie-plugin-users": "^2.1.0",
    "hoodie-server": "^4.0.3",
    "mithril": "^0.2.0",
    "mithril.elements": "^0.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "eslint": "^1.6.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "grunt": "^0.4.5",
    "grunt-concurrent": "^2.0.3",
    "jspm": "^0.16.12",
    "jss": "^2.3.5",
    "standard": "^5.3.1"
  },
  "engines": {
    "node": ">=0.10.22"
  },
  "hoodie": {
    "plugins": [
      "hoodie-plugin-appconfig",
      "hoodie-plugin-email",
      "hoodie-plugin-users"
    ]
  },
  "_shasum": "2cff3f5f59e47bc458dd4eb54d68dd6ad52bf4a0",
  "_resolved": "https://registry.npmjs.org/my-first-hoodie/-/my-first-hoodie-3.0.0.tgz",
  "readme": "README.md",
  "jspm": {
    "directories": {
